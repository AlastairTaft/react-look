{
  "name": "react-look",
  "version": "0.3.3",
  "description": "Feature-rich, dynamic & stateful Component Styling for React",
  "main": "lib/index.js",
  "files": [
    "LICENSE",
    "README.md",
    "lib/",
    "docs/"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/rofrischmann/react-look"
  },
  "scripts": {
    "pretest": "npm run transpile",
    "test": "mocha --recursive --compilers js:babel/register",
    "transpile": "babel src --out-dir lib",
		"release" : "npm run transpile && npm publish",
    "build:demo": "browserify ./demo/app.jsx -t babelify -d -p [minifyify --no-map] > ./demo/bundle.js",
    "demo": "npm run build:demo && open demo/index.html"
  },
  "keywords": [
    "react",
    "layout",
    "component styling",
    "styling",
    "vendor",
    "stylesheet",
    "media query",
    "inline-style",
    "stateful style"
  ],
  "author": "Robin Frischmann",
  "license": "MIT",
  "dependencies": {
    "assign-styles": "^1.0.2",
    "dynamic-style-sheets": "^0.2.2",
    "type-utils": "^1.0.4"
  },
  "devDependencies": {
    "babel": "^5.6.14",
    "babelify": "^6.1.3",
    "browserify": "^10.2.6",
    "chai": "^3.2.0",
    "color": "^0.10.1",
    "minifyify": "^7.0.5",
    "mocha": "^2.2.5",
    "react": "^0.13.3",
    "react-layout-components": "^0.1.0",
    "sinon": "^1.15.4",
    "sinon-chai": "^2.8.0"
  }
}
