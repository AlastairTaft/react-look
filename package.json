{
  "name": "react-look",
  "version": "0.3.2",
  "description": "Feature-rich, dynamic & stateful Component Styling for React (CSS in JS)",
  "main": "lib/index.js",
  "files": [
    "LICENSE",
    "README.md",
    "lib/",
    "docs/"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/rofrischmann/react-look"
  },
  "scripts": {
    "pretest": "babel src --out-dir lib",
    "test": "mocha --recursive --compilers js:babel/register",
    "start": "browserify ./demo/app.jsx -t babelify --outfile ./demo/bundle.js",
    "poststart": "open demo/index.html",
    "prepublish": "babel src --out-dir lib"
  },
  "keywords": [
    "react",
    "layout",
    "component styling",
    "styling",
    "vendor",
    "stylesheet",
    "media query",
    "inline styles",
    "stateful styles"
  ],
  "author": "Robin Frischmann",
  "license": "MIT",
  "dependencies": {
    "assign-styles": "^1.0.2",
    "dynamic-style-sheets": "^0.2.2",
    "type-utils": "^1.0.4"
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "devDependencies": {
    "babel": "^5.6.14",
    "babelify": "^6.1.3",
    "browserify": "^10.2.6",
    "chai": "^3.2.0",
    "color": "^0.10.1",
    "dss-mixins": "0.0.6",
    "mocha": "^2.2.5",
    "react": "^0.13.3",
    "react-flex-kit": "^0.1.3",
    "react-router": "^0.13.3",
    "sinon": "^1.15.4"
  }
}
