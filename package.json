  "repository": "https://github.com/rofrischmann/react-look",
  "scripts": {
    "babel": "npm run clean && babel -d lib modules",
    "bundle": "npm run babel && babel-node build/preparePackages.js",
    "clean": "rimraf lib coverage packages/react-look/lib packages/react-look-native/lib",
    "coverage": "codeclimate-test-reporter < coverage/lcov.info",
    "demo": "webpack-dev-server --config demo/webpack.config.js --no-info --content-base demo/",
    "demo:universal": "concurrent --kill-others \"npm run demo\" \"babel-node demo/server.js\"",
    "start": "node build/devServer.js",
    "lint": "eslint modules",
    "package": "npm test && npm run bundle",
    "release": "npm run package && npm run release:dom && npm run release:native",
    "release:dom": "cd packages/react-look && npm publish",
    "release:native": "cd packages/react-look-native && npm publish",
    "test": "istanbul cover _mocha -- --opts test/mocha.opts"
  },
  "devDependencies": {
    "assign-styles": "^2.0.0",
    "babel": "^6.3.26",
