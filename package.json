{
  "name": "react-look",
  "version": "0.4.9",
  "description": "Processable, dynamic & stateful Component Styling for React & React Native",
  "main": "look.js",
  "files": [
    "LICENSE",
    "README.md",
    "dom.js",
    "look.js",
    "core.js",
    "lib/",
    "docs/"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/rofrischmann/react-look"
  },
  "scripts": {
    "clean": "rimraf lib dist coverage",
    "pretest": "npm run transpile",
    "test": "mocha --recursive --compilers js:babel/register",
    "test:watch": "mocha --recursive --compilers js:babel/register -w",
    "test:coverage": "istanbul cover _mocha -- --recursive --compilers js:babel/register",
    "coverage": "cat ./coverage/coverage.json | ./node_modules/codecov.io/bin/codecov.io.js",
    "transpile": "npm run clean && babel src --out-dir lib",
    "package": "npm run transpile && node build-package.js",
    "release": "npm run package && npm publish",
    "demo": "npm run build:demo && open demo/index.html",
    "demo-dev": "npm run build:demo-dev && open demo/index.html",
    "bundle": "npm run transpile && mkdir dist && npm run bundle:core && npm run bundle:look && npm run bundle:dom",
    "bundle:core": "browserify src/core.js -t babelify --external react -d -p  [minifyify --no-map] > ./dist/look-core.min.js",
    "bundle:dom": "browserify src/dom.js -t babelify --external react -d -p  [minifyify --no-map] > ./dist/look-dom.min.js",
    "bundle:look": "browserify src/look.js -t babelify --external react -d -p  [minifyify --no-map] > ./dist/look.min.js",
    "build:demo": "npm run transpile && browserify ./demo/app.jsx -t babelify -d -p [minifyify --no-map] > ./demo/bundle.js",
    "build:demo-dev": "npm run transpile && browserify ./demo/app.jsx -t babelify --outfile ./demo/bundle.js"
  },
  "keywords": [
    "react",
    "layout",
    "react-native",
    "ios",
    "android",
    "component styling",
    "styling",
    "vendor",
    "stylesheet",
    "media query",
    "inline-style",
    "stateful style",
    "cssinjs"
  ],
  "author": "Robin Frischmann",
  "license": "MIT",
  "dependencies": {
    "assign-styles": "^2.0.0",
    "dynamic-style-sheets": "^0.2.2",
    "inline-style-prefixer": "^0.3.2",
    "object-assign": "^4.0.1"
  },
  "devDependencies": {
    "babel": "^5.8.23",
    "babelify": "^6.3.0",
    "browserify": "^11.2.0",
    "chai": "^3.3.0",
    "codecov.io": "^0.1.6",
    "color": "^0.10.1",
    "istanbul": "^0.3.22",
    "minifyify": "^7.1.0",
    "mocha": "^2.3.3",
    "rimraf": "^2.4.3",
    "sinon": "^1.17.1",
    "sinon-chai": "^2.8.0",
    "react": "0.14.0",
    "react-dom": "0.14.0"
  },
  "peerDependencies": {
    "react-native": "0.11"
  }
}
