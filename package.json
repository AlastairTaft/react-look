{
  "name": "react-look",
  "version": "0.7.2",
  "description": "Advanced & Dynamic Component Styling for React & React Native",
  "main": "look.js",
  "files": [
    "LICENSE",
    "README.md",
    "dom.js",
    "look.js",
    "core.js",
    "addons.js",
    "lib/",
    "docs/",
    "dist/"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/rofrischmann/react-look"
  },
  "scripts": {
    "babel": "npm run clean && babel -d lib modules",
    "bundle": "npm run babel && mkdir dist && npm run bundle:core && npm run bundle:look && npm run bundle:dom && npm run bundle:addons",
    "bundle:addons": "browserify modules/addons.js -t babelify --external react -d -p  [minifyify --no-map] > ./dist/look-addons.min.js",
    "bundle:core": "browserify modules/core.js -t babelify --external react -d -p  [minifyify --no-map] > ./dist/look-core.min.js",
    "bundle:dom": "browserify modules/dom.js -t babelify --external react -d -p  [minifyify --no-map] > ./dist/look-dom.min.js",
    "bundle:look": "browserify modules/look.js -t babelify --external react -d -p  [minifyify --no-map] > ./dist/look.min.js",
    "clean": "rimraf lib dist coverage",
    "coverage": "codeclimate-test-reporter < coverage/lcov.info",
    "demo": "webpack-dev-server --config demo/webpack.config.js --no-info --content-base demo/",
    "demo:universal": "concurrent --kill-others \"npm run demo\" \"npm start\"",
    "start": "node devServer.js",
    "lint": "eslint modules",
    "package": "npm test && npm run bundle && node build-package.js",
    "release": "npm run package && npm publish",
    "test": "mocha --recursive --compilers js:babel-core/register",
    "test:coverage": "babel-node ./node_modules/isparta/bin/isparta cover ./node_modules/mocha/bin/_mocha -- --recursive"
  },
  "keywords": [
    "react",
    "layout",
    "react-native",
    "react-component",
    "ios",
    "android",
    "component styling",
    "styling",
    "vendor",
    "stylesheet",
    "media query",
    "inline-style",
    "stateful style",
    "cssinjs"
  ],
  "author": "Robin Frischmann",
  "license": "MIT",
  "dependencies": {
    "assign-styles": "2.0.0",
    "dot-prop": "^2.2.0",
    "inline-style-linter": "^0.2.7",
    "inline-style-prefixer": "^0.6.3",
    "inline-style-transformer": "^1.0.0",
    "object-assign": "^4.0.1",
    "object-assign-deep": "0.0.4"
  },
  "peerDependencies": {
    "react-native": "^0.17.0",
    "react": "^0.14.3",
    "react-dom": "^0.14.3"
  },
  "devDependencies": {
    "babel": "^6.3.26",
    "babel-cli": "^6.4.0",
    "babel-core": "^6.4.0",
    "babel-eslint": "^4.1.6",
    "babel-loader": "^6.2.1",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-react-display-name": "^6.4.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-react-hmre": "^1.0.1",
    "babel-preset-stage-0": "^6.3.13",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "chai": "^3.4.1",
    "codeclimate-test-reporter": "^0.1.1",
    "color": "^0.11.1",
    "concurrently": "^1.0.0",
    "cross-env": "^1.0.7",
    "eslint": "^1.10.3",
    "eslint-config-rackt": "^1.1.1",
    "eslint-plugin-react": "^3.15.0",
    "express": "^4.13.3",
    "express-http-proxy": "^0.6.0",
    "isparta": "^4.0.0",
    "jsdom": "^7.2.2",
    "json-loader": "^0.5.4",
    "minifyify": "^7.1.0",
    "mocha": "^2.3.4",
    "path": "^0.12.7",
    "react": "^0.14.6",
    "react-addons-test-utils": "^0.14.6",
    "react-dom": "^0.14.6",
    "rimraf": "^2.5.0",
    "sinon": "^1.17.2",
    "sinon-chai": "^2.8.0",
    "webpack": "^1.12.11",
    "webpack-dev-middleware": "^1.5.0",
    "webpack-dev-server": "^1.14.1",
    "webpack-hot-middleware": "^2.6.0"
  }
}
