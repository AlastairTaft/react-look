{
  "name": "react-look",
  "version": "0.7.0",
  "description": "Advanced & Dynamic Component Styling for React & React Native",
  "main": "look.js",
  "files": [
    "LICENSE",
    "README.md",
    "dom.js",
    "look.js",
    "core.js",
    "addons.js",
    "lib/",
    "docs/"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/rofrischmann/react-look"
  },
  "scripts": {
    "bundle": "npm run babel && mkdir dist && npm run bundle:core && npm run bundle:look && npm run bundle:dom && npm run bundle:addons",
    "bundle:addons": "browserify modules/addons.js -t babelify --external react -d -p  [minifyify --no-map] > ./dist/look-addons.min.js",
    "bundle:core": "browserify modules/core.js -t babelify --external react -d -p  [minifyify --no-map] > ./dist/look-core.min.js",
    "bundle:dom": "browserify modules/dom.js -t babelify --external react -d -p  [minifyify --no-map] > ./dist/look-dom.min.js",
    "bundle:look": "browserify modules/look.js -t babelify --external react -d -p  [minifyify --no-map] > ./dist/look.min.js",
    "clean": "rimraf lib dist coverage",
    "coverage": "cat ./coverage/coverage.json | ./node_modules/codecov.io/bin/codecov.io.js",
    "demo": "webpack-dev-server --config demo/webpack.config.js --no-info --content-base demo/",
    "demo:universal": "concurrent --kill-others \"npm run demo\" \"npm start\"",
    "demo:dev": "npm run babel && npm run demo",
    "start": "babel-node demo/server.js",
    "lint": "eslint modules",
    "package": "npm run test && npm run bundle && node build-package.js",
    "pretest": "npm run babel",
    "release": "npm run package && npm publish",
    "test": "npm run lint && mocha --recursive --compilers js:babel-core/register",
    "test:coverage": "istanbul cover _mocha -- --recursive --compilers js:babel-core/register",
    "test:watch": "mocha --recursive --compilers js:babel-core/register -w",
    "babel": "npm run clean && babel -d lib modules"
  },
  "keywords": [
    "react",
    "layout",
    "react-native",
    "react-component",
    "ios",
    "android",
    "component styling",
    "styling",
    "vendor",
    "stylesheet",
    "media query",
    "inline-style",
    "stateful style",
    "cssinjs"
  ],
  "author": "Robin Frischmann",
  "license": "MIT",
  "dependencies": {
    "assign-styles": "2.0.0",
    "dot-prop": "^2.2.0",
    "inline-style-prefixer": "^0.5.4",
    "object-assign": "^4.0.1",
    "object-assign-deep": "0.0.4"
  },
  "devDependencies": {
    "babel": "^6.1.18",
    "babel-cli": "^6.2.0",
    "babel-core": "^6.2.1",
    "babel-eslint": "^4.1.6",
    "babel-loader": "^6.2.0",
    "babel-plugin-add-module-exports": "^0.1.1",
    "babel-plugin-transform-decorators": "^6.1.18",
    "babel-plugin-transform-react-display-name": "^6.1.18",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.1.18",
    "babel-preset-stage-0": "^6.1.18",
    "babelify": "^7.2.0",
    "browserify": "^12.0.1",
    "chai": "^3.4.1",
    "codecov.io": "^0.1.6",
    "color": "^0.10.1",
    "concurrently": "^1.0.0",
    "eslint": "^1.10.2",
    "eslint-config-airbnb": "^1.0.2",
    "eslint-plugin-react": "^3.11.2",
    "express": "^4.13.3",
    "express-http-proxy": "^0.6.0",
    "istanbul": "^0.4.1",
    "json-loader": "^0.5.4",
    "minifyify": "^7.1.0",
    "mocha": "^2.3.4",
    "path": "^0.12.7",
    "react": "^0.14.3",
    "react-dom": "^0.14.3",
    "rimraf": "^2.4.4",
    "sinon": "^1.17.2",
    "sinon-chai": "^2.8.0",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.0"
  },
  "peerDependencies": {
    "react-native": "^0.14.0",
    "react": "^0.14.3",
    "react-dom": "^0.14.3"
  }
}
